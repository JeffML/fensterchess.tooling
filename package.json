{
  "name": "fensterchess.tooling",
  "version": "0.1.0",
  "description": "Data maintenance tooling for fensterchess master games database",
  "type": "module",
  "scripts": {
    "download": "tsx scripts/downloadMasterGames.ts",
    "download:pgnmentor": "tsx scripts/downloadPgnmentor.ts",
    "build-indexes": "tsx scripts/buildIndexes.ts",
    "backup": "tsx --env-file=.env scripts/backupFromBlobs.ts",
    "upload": "node --env-file=.env scripts/uploadToBlobs.js",
    "test:connection": "node --env-file=.env scripts/testBlobConnection.js",
    "test:filters": "node test/testFiltering.js",
    "test:chunks": "tsx test/testChunkLogic.ts",
    "test:pipeline": "tsx test/testPipeline.ts",
    "test:batch": "vitest run test/batchMetadata.test.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "chess",
    "pgn",
    "database",
    "tooling"
  ],
  "author": "Jeff Lowery",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JeffML/fensterchess.tooling.git"
  },
  "dependencies": {
    "@chess-openings/eco.json": "^1.1.0",
    "@chess-pgn/chess-pgn": "^1.0.0",
    "@netlify/blobs": "^8.1.0",
    "adm-zip": "^0.5.16",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
